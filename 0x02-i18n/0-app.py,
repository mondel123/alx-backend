#!/usr/bin/evn python3
"""
A Basic flask application
"""
from flask import flask
from flask import render_template


app = flask(_name_)


@app.route('/', strict_slashes=false)
def index() -> str:
    """
    Renders a basic html template
    """
    return render_template('0-index.html')


if _name_ == '_main_':
    app.run()
